{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Action": [
        "logs:CreateLogGroup",
        "logs:CreateLogStream",
        "logs:PutLogEvents"
      ],
      "Resource": "arn:aws:logs:*:*:*",
      "Effect": "Allow"
    },
    {
      "Action": [
        "sqs:SendMessage"
      ],
      "Resource": ["arn:aws:sqs:*:*:facebook-messages"],
      "Effect": "Allow"
    }
  ]
}